<?xml version="1.0" encoding="UTF-8"?>
<mp:servoy-extension xmlns:mp="http://www.servoy.com/extension" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.servoy.com/extension http://www.servoy.com/schema/servoy-extension-1.xsd">
	<extension-id>com.servoy.rawSQL</extension-id>
	<extension-name>RawSQL Plugin</extension-name> 
	<version>6.1.0</version>
	<dependencies>
	    <servoy>
	       <min-version>6.1.0 rc1</min-version>
	    </servoy>
	</dependencies>
	<info>
		<icon path="images/sql.gif"/>
		<description>Plugin for executing any SQL commands (assuming that you have the required level of access), including inserts, updates, deletes. You can flush the Client database cache and you can get the exception message for a failed statement execution.</description>
	</info>
	<requiresRestart/>
</mp:servoy-extension>